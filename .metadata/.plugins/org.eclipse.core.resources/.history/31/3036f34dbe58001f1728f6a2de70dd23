public class Lab01_4 {
	
	public static void main(String[] args) {
		
		System.out.println("I       C     EEEEEEE	        C	RRRRR       EEEEEEEE           A       ");
		System.out.println("I      C      E	               C	R   RR      E                 A A     ");
		System.out.println("I     C       E	              C		R   RRR     E                A   A     ");
		System.out.println("I    C        E	     	     C		R   RR      E               A     A    ");
		System.out.println("I   C         EEEEEEE	    C		RRRRR       EEEEEEEE       AAAAAAAAA         ");
		System.out.println("I    C        E      	     C		R R         E             A         A      ");
		System.out.println("I     C       E      	      C		R  R        E            A           A");
		System.out.println("I      C      E      	       C	R   R       E           A             A");
		System.out.println("I       C     EEEEEEE	        C	R    R      EEEEEEEE   A               A");

	}

}
