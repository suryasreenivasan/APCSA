package dum;
import java.util.*;
//09/08/2024
public class natdum {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		double x = scanner.nextDouble();
		double y = scanner.nextDouble();
		scanner.close();
		System.out.println(x + y);
		
		// java strength - strongly typed 
		// weakness - interpreted; needs to read the code and translate it
		// johan neumann - computer should input, storage + process, output 
	}

}
