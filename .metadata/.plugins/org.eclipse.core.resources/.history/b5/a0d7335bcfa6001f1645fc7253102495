
public class BigVig {

	public static void main(String[] args) {
		

	}
	
	public static boolean cheque(int[] arr) {
		boolean fifty54 = false;
		boolean thirty6 = false;
		for (int i = 0; i < arr.length; i++) {
			if (arr[i] == 54) fifty54 = true;
			if (arr[i] == 36) thirty6 = true;
		}
		return fifty54 && thirty6;
	}

}
