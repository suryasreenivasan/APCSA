import java.util.*;
public class Compute_Money {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Enter the amount of money: $");
		double money = input.nextDouble();
		money  = (int) 100* money;
		input.close();
		
		int dollars = (int) (money/100);
		money  = money % 100;
		int quarters = (int) (money/25);
		money = money % 25;
		int dimes = (int) (money/10);
		money = money % 10;
		int nickels = (int) (money/5);
		money = money % 5;
		int pennies = (int) (money);
		
		System.out.println("There are "+dollars + " dollars");
		System.out.println("There are "+quarters+" quarter(s)");
		System.out.println("There are "+dimes+" dime(s)");
		System.out.println("There are "+nickels+" nickel(s)");
		System.out.println("There are "+pennies+" pennies");
	}

}