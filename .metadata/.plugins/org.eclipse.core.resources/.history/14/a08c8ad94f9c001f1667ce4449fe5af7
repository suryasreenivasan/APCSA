
public class Arr {

	public static void main(String[] args) {
		int[] arr = {1, 2, 3, 4, 5};
		ProcessArray.printArr(shiftToRight(arr));

	}
	
	public static int[] shiftToRight(int[] a) {
		int[] result = new int[a.length];
		int temp = a[0];
		for(int i = 0; i < a.length - 1; i++) {
			result[i] = a[i+1];
		}
		result[a.length - 1] = temp;
		return result;
	}

}
