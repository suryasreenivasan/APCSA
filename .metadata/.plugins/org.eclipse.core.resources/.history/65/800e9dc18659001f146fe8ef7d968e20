	
	public class Practice2 {
	
		public static void main(String[] args) {
		
			System.out.println("what is the radius of a circle");
			double radius = 2;
			double area;
			
			(area) = radius * radius * 3.14159265;
			
			
		System.out.println("area of circle with radius " + radius + " is " + (Math.round(area)));
		}
	
	}
