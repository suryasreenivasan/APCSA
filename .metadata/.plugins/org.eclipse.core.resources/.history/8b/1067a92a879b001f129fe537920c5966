
public class PracticeArray {

	public static void main(String[] args) {
		int[] list1 = {1, 2, 3, 4, 5};
		int[] list2 = list1;
		doubleArray(list1);
		ProcessArray.printArr(list1);
		ProcessArray.printArr(list2);
		
	}
	
	public static void doubleArray(int[] a) {
		for (int i = 0; i < a.length; i++) {
			a[i] *= 2;
		}
	}
	
}
