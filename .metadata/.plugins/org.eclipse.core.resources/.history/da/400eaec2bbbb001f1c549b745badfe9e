// january the fifth
public class Rational {
	private int numerator = 0;
	private int denominator = 1;
	public Rational() {
		
	}
	public Rational(int n, int d) {
		if (d == 0) {
			System.out.println("denominator cannot be zero");
			System.exit(0);
		}
		numerator = n;
		denominator = d;
	}
	public int getNumerator() {
		return numerator;
	}
		
}