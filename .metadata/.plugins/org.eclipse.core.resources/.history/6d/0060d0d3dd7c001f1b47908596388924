
public class PrimeNumbers {

	public static void main(String[] args) {
		final int NUM_OF_PRIME = 50;
		int count = 0;
		int prime = 2;
		String numbers = "";
		while (count < NUM_OF_PRIME) {
			boolean isPrime = true;
			for (int k = 2; k <= (prime/2); k++) {
				if ((k % 2) == 0 ) {
						isPrime = false;
						break;
				}
				prime++;
			}
			if (isPrime) {
				count++; 
				if ((prime % 10) == 0) {
					System.out.println(prime);
				}
				else {
					System.out.println(prime + " ");
				}
			}
			prime++;
			
		}
		
	}

}
