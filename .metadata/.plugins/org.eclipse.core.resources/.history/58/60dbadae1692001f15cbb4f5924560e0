import java.util.*;
public class SecurePassword {
	
	public static void main(String[] args) {
		
	}
	
	public static boolean isValid(String s) {
		if (s.length() < 8) return false;
		int countDigit = 0;
		for (int i = 0; i < s.length(); i++) {
			String temp = s.substring(i, i+1);
		}
		return true;
		
	}
}
