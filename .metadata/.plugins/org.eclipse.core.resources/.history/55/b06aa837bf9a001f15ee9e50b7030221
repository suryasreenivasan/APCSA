
public class CopyArray {

	public static void main(String[] args) {
		int[] list1 = new int[5];
		for (int i = 0; i < list1.length; i++) {
			list1[i] = i + 1;
		}
		int[] list2 =  list1;
		list1[0] = -100; 
		printArr(list2);

	}

	public static void printArr(int[] a) {
		for (int e: a) {
			System.out.print(e + " ");
		}
	}
}
