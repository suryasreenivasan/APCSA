
public class CoinFlip {

	public static void main(String[] args) {
		
	threeHeads();
	}
	
	public static void threeHeads() {
		int count = 0;
		while (count < 3) {
			int flip = (int)(Math.random());
			System.out.println(flip);
			if (flip == 1) {
				System.out.print("H ");
				count++;
			}
			else {
				System.out.print("T ");
				count = 0;
			}
			
		}
	}

}
