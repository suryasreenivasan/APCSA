import java.util.Scanner;

public class MissingNumber {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        // Read the size of the sequence
        int n = scanner.nextInt();

        // Calculate the total sum of numbers from 1 to n
        long totalSum = (long) n * (n + 1) / 2;

        // Calculate the sum of the given numbers
        long givenSum = 0;
        for (int i = 0; i < n - 1; i++) {
            givenSum += scanner.nextInt();
        }

        // The missing number is the difference between the total sum and the given sum
        long missingNumber = totalSum - givenSum;

        // Print the missing number
        System.out.println(missingNumber);
        scanner.close();
    }
}
