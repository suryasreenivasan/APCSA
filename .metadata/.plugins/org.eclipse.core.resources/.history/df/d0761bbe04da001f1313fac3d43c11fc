import java.util.*;
public class Poker {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		System.out.println("play five cards!");
		while (true) {
			System.out.println("Scores for one-pair:  1");
			System.out.println("\t   two-pair: ");
			System.out.println("\t   triple:    3");
			System.out.println("\t   full house: 4");
			break;
			
		}

	}

}
