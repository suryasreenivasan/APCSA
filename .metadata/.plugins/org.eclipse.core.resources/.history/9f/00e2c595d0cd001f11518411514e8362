import java.util.Arrays;

public class StackOfIntegers {
	
	private int[] elements;
	private int size;
	public static final int DEFAULT_CAPACITY = 16;
	
	public StackOfIntegers(int capacity) {
		elements = new int[capacity];
	}
	
	public StackOfIntegers() {
		this(DEFAULT_CAPACITY);
	}
	
	public int getSize() {
		return size;
	}
	
	public boolean empty() {
		return size == 0;
	}
	
	public int peek() {
	return elements[size - 1];
	}
	
	public void push (int n) {
		if (size == elements.length) {
			int[] newList = new int[elements.length * 2];
			for (int i = 0; i < elements.length; i++) {
				newList[i] = elements[i];
			}
			elements = newList;
		}
		elements[size++] = n;
		
	}
	
	public int pop() {
		return elements[--size];
	}
	public String toString() {
		return Arrays.toString(elements);
	}
	
	public static void main(String[] args) {
		//write a program that displays all the prime numbers less than 120 in decreasing order
		StackOfIntegers primeStack = new StackOfIntegers();
		final int limit = 120;
		int count = 0;
		for (int i = 0; i < limit; i++) {
			if (isPrime(i)) {
				primeStack.push(i);
				count++;
			}
		}
		System.out.println("there are " + count + " prime's under " + limit);
		count = 0;
		while(!primeStack.empty()) {
			System.out.print(primeStack.pop());
			count++;
			if (count % 10 == 0) {
				System.out.println();
			}
			else {
				System.out.println("   ");
			}
		}
		
	}
	 public static boolean isPrime(int a) {
	        if (a == 0 || a == 1) {
	            return false;
	        }
	        for (int i = 2; i <= a / 2; i++) {
	            if (a % i == 0) {
	                return false;
	            }
	        }
	        return true;
	    }

}
