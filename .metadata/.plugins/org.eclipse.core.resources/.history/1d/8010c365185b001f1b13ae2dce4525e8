import java.util.*;
public class PracticeIntDivision {

	public static void main(String[] args) {
		
		double n1 = 1;
		double n2 = 2;
		double n3 = 3;
		double average = ((n1 + n2 + n3)/3);
		System.out.println("Average: " + average);
		
		int n4 = 1;
		int n5 = 2;
		int n6 = 3;
		int intaverage = ((n4 + n5 + n6)/2);
		System.out.println("Average int: " + intaverage);

	
		
	}
	
}
