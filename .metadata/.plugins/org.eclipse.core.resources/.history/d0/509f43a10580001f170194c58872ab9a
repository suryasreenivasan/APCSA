import java.util.*;
public class Practice {

	public static void main(String[] args) {
		int sum = 0;
		for (int i = 1; i <= 1000; i++) {
		sum += i;	
		}
		System.out.println("Sum from 1 to 1000: " + sum);
		sum = 0;
		for (int i = 1; i <= 1000; i++) {
			if ((i % 2) == 0) {
				sum += i;
			}
		}
		System.out.println("Even sum from 1 to 1000 " + sum);
	}

}
