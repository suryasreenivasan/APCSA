import java.util.*;
public class Crap {

	public static void main(String[] args) {
		int dice = getDice();
	}
	
	public static int getDice() {
		Random random = new Random();
		int die1 = random.nextInt(6)+1;
		int die2 = random.nextInt(6) + 1;
		System.out.println("You rolled a " + die1 + " and a " + die2);
		return die1 + die2;
	}

}
