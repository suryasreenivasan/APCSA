public class Lab01_4 {
	
	public static void main(String[] args) {
		
		System.out.println("I       C     EEEEEEE	        C	    RRRRR		E");
		System.out.println("I      C      E	               C	R	RR		E");
		System.out.println("I     C       E	     		  C		R	RRRR	E");
		System.out.println("I    C        E	     	     C		R	RR		E");
		System.out.println("I   C         EEEEEEE	    C		RRRRR		E");
		System.out.println("I    C        E      	     C		R R			E");
		System.out.println("I     C       E      	      C		R	R		E");
		System.out.println("I      C      E      	       C	    R	  R		E");
		System.out.println("I       C     EEEEEEE	        C	R	    R	E");

	}

}
