import java.util.*;

public class FreeRobuxTutorialWithSachin {
	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		try {
		System.out.println("enter a number: ");
		double a = scanner.nextDouble();
		System.out.println("enter another number: ");
		double b = scanner.nextDouble();
		System.out.println("enter a symbol to mathematical");
		char e = scanner.next().charAt(0);
		System.out.println(FixBasicCalculator(a, b, e));
		
		}
		catch (Exception e) {
			System.out.println("end your own lfie");
		}
		scanner.close();
		
	}
	
	public static String FixBasicCalculator(double a, double b, char e) {
		double c = 0;
		try {
		switch (e) {
		case '+':  c = a + b; break;
		case '-': c = a - b; break;
		case '/': {
			if (b != 0) {
				c = a / b;
			}
			else {
				return "null";
			}
			break;
		}
		case '*': c = a * b; break;
		default: return "null";
		}
		String d = c + "";
		return d;
		}
		catch (Exception E) {
			return "null";
		}
		
	}
}
