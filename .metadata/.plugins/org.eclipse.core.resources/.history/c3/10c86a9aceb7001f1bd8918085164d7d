
public class TV {

	private int currentChannel = 1; // 1-120
	private int volumeLevel = 1; // 1-7
	private boolean on = false;
	
	public TV() {
		
	}
	
	void turnOn() {
		on = true;
	}
	
	void turnOff() {
		on = false;
	}
	
	void setChannel(int newChannel) {
		currentChannel = newChannel;
	}
	
	void setVolume(int newVolume) {
		volumeLevel = newVolume;
	}

}
