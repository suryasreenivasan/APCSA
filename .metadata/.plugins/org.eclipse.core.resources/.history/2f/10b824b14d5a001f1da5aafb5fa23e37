import java.util.*;

public class Practice3 {

	public static void main(String[] args) {
		
		double radius = 1;
		double area = radius * radius * 3.14;
		System.out.println("The area of a circle with radius " + radius + " is " + area);
		
		radius = 10;
		System.out.println(The area of a circle with radius " + radius + " is " + area);
	}

}
