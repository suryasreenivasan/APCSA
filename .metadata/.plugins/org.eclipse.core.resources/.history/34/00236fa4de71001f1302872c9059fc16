import java.util.*;
public class order3cities {

	public static void main(String[] args) {
		System.out.println("Enter city 1: ");
		Scanner scanner = new Scanner(System.in);
		String city1 = scanner.nextLine().toUpperCase();
		System.out.println("Enter city 2: ");
		String city2 = scanner.nextLine().toUpperCase();
		System.out.println("Enter city 3: ");
		String city3 = scanner.nextLine().toUpperCase();
		scanner.close();
		
		int difference = city2.compareTo(city1);
		if (difference > 0) {
			String temp = city2; 
			city2 = city1; 
			city1 = temp;
		}
		difference = city3.compareTo(city2);
		if (difference > 0) {
			String temp = city3;
			city3 = city2;
			city2 = temp;
		}
		
			
		
		System.out.println("3 cities in alphabetical order: " + city1 + " " + city2 + " " + city3);
		
	}

}
