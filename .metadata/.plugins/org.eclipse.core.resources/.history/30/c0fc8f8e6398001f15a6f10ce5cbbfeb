
public class LargestElement {
	
	public static void main(String[] args) {
		
		int[] array = RandomArrays.randomArray(10);
		int max = array[0];
			for (int i = 0; i < array.length; i++) {
				if (array[i] > max) {
					max = array[i];
				}
			}
		System.out.println("the maximum of array ");
		ProcessArray.printArr(array);
		System.out.println("is " + max);
	}
	
}
