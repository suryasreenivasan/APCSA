package dum;
import java.util.*;
public                                                                                                    class 



depths {
	public 
	static 
	void main
	(String[]
			args
			) {
		int count = 0;
		int n = 1;
		for (n = 1; n < 100; n -= 10000000) {
			System.out.println("this is an infinite for loop that has been repeated " + count + " times");
			count ++;
		}
		System.out.println(n);
	}
}