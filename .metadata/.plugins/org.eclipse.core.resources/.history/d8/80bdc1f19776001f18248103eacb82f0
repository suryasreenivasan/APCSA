import java.util.*;
public class floatingpoint {

	public static void main(String[] args) {
		System.out.println("Enter a floating point number: ");
		Scanner scanner = new Scanner(System.in);
		String user = scanner.nextLine();
		scanner.close();
		
		String number = user + "";
		int digits = number.indexOf(".");
		
		System.out.println("");

	}

}
